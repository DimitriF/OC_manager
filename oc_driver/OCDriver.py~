from printrun.printcore import printcore

import FineControlDriver


class OCDriver:
    def __init__(self, connectionString, baudRate):
        self.printcore = printcore()
        printcore.connect(connectionString, baudRate)
        printcore.listen_until_online()

        self.fine_control_driver = FineControlDriver()
        


    def get_fine_control_driver():
        return self.fine_control_driver
